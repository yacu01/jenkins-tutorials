pipeline {
    agent any
    stages {
        stage('clean') {
            steps {
                script {
                    try {

                        sh '''
                        source /Users/ryacu/.zprofile
                        echo $PATH
                        cd /Users/ryacu/AnypointStudio/irs/irs-taxpayer-papi
                        mvn clean
                        sh '''
                    } catch ( Exception e ) {
                        echo 'Exception occurred: ' + e.toString()
                    }
                }
            }
         stage('package') {
            steps {
                script {
                    try {

                        sh '''
                        mvn package -DskipTests
                        sh '''
                    } catch ( Exception e ) {
                        echo 'Exception occurred: ' + e.toString()
                    }
                }
            }
        }
    }
}
