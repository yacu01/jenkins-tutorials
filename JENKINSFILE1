pipeline {
    agent any
    stages {
        stage('setup') {
            steps {
                script {
                    try {
                        sh '''
                        source /Users/ryacu/.zprofile
                        echo $PATH
                        cd /Users/ryacu/AnypointStudio/irs/irs-taxpayer-papi
                        sh '''
                    } catch ( Exception e ) {
                        echo 'Exception occurred: ' + e.toString()
                    }
                }
            }
         }
         stage('package') {
            steps {
                script {
                    try {

                        sh '''
                        source /Users/ryacu/.zprofile
                        echo $PATH
                        cd /Users/ryacu/AnypointStudio/irs/irs-taxpayer-papi
                        mvn clean package -DskipTests
                        sh '''
                    } catch ( Exception e ) {
                        echo 'Exception occurred: ' + e.toString()
                    }
                }
            }
        }
        stage('deploy') {
            steps {
                script {
                    try {
                        sh ''' 
                        echo Deploying ...
                        sh '''
                    } catch( Exception e) {
                        echo 'Exception occurred: ' + e.toString()     
                    }
                }
            }
        }
    }
}
